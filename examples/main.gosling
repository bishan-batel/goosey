import std.io.*
import "stdio.h"

export fun main(argc: i32, argv: ref[ref[char]])  {
	let num = 0

	unless i32::try_parse(ref num) {
		println!("{num} is Invalid")
		return
	}

	let fizz = num % 3 == 0
	let buzz = num % 5 == 0 
	
	unless fizz || buzz {
		println!("{num}")
	} else {
		if fizz then print!("Fuzz")
		if buzz then print!("buzz")
		println!()
	}
}
